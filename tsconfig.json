{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": ["dom", "esnext"],
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"]
        }
    },
    "include": ["src"]
}